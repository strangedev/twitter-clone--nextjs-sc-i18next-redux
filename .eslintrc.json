{
  "extends": [
    "next/core-web-vitals",
    "es/browser"
  ],
  "rules": {
    "no-underscore-dangle": "off",
    "@typescript-eslint/explicit-function-return-type": [
      "error",
      {
        "allowTypedFunctionExpressions": true
      }
    ]
  },
  "ignorePatterns": [
    "next-env.d.ts",
    "next.config.js",
    "next-i18next.config.js"
  ]
}
